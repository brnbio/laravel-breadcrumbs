{
    "name": "brnbio/laravel-breacdcrumbs",
    "description": "Breadcrumb helper for laravel framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Heider",
            "email": "heider@brnb.io"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Brnbio\\LaravelBreadcrumbs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brnbio\\LaravelBreadcrumbs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brnbio\\LaravelBreadcrumbs\\BreadcrumbsServiceProvider"
            ]
        }
    }
}
